{
  "index": "/",
  "appData": {
    "name": "The standalone EK-Shop application",
    "description": "The alpha stage standalone progressive web application "
  },
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/assets/icons/favicon.ico",
        "/index.html",
        "/ngsw-worker.js",
        "/*.client.js",
        "/client.js"
      ],
        "versionedFiles": [
            "/main.css"
        ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/img/**",
        "/styles/**"
      ]
    }
  }, {
    "name": "fonts",
    "resources": {
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**",
        "https://cdn.ampproject.org/**"
      ]
    }
  }]
}
